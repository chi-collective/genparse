start: WS sum "</s>"
        | NAME "=" sum "</s>"  -> assign_var

sum: product
    | sum "+" product   -> add
    | sum MINUS product   -> sub

product: atom
    | product "*" atom  -> mul
    | product "/" atom  -> div

atom: NUMBER           -> number
        | MINUS atom         -> neg
        | NAME             -> var
        | "(" sum ")"

MINUS: /[\-]/
NUMBER: /[\-+]?\d{1,3}(\.\d{1,3})?/
WS: /[ \t\f\r\n]/
NAME: /[a-zA-Z_]{1,5}/
